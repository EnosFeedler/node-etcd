{
  "name": "node-etcd",
  "version": "0.2.0",
  "description": "etcd library for node.js",
  "licenses": [
    {
      "type": "BSD 3-Clause",
      "url": "http://opensource.org/licenses/BSD-3-Clause"
    }
  ],
  "maintainers": [
    {
      "name": "Stian Eikeland",
      "email": "stian@eikeland.se",
      "web": "http://eikeland.se/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/stianeikeland/node-etcd.git"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/coffee --bare --compile --output ./lib ./src/*.coffee",
    "build": "./node_modules/.bin/coffee --bare --compile --output lib/ src/*.coffee",
    "prepublish": "./node_modules/.bin/coffee --bare --compile --output ./lib ./src/*.coffee",
    "postpublish": "rm -rf lib",
    "test": "node_modules/mocha/bin/mocha --compilers coffee:coffee-script"
  },
  "dependencies": {
    "underscore": "*",
    "request": "*",
    "coffee-script": "*",
    "mocha": "*",
    "should": "*",
    "nock": "*"
  },
  "keywords": ["etcd"]
}
